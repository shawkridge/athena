# Athena Dashboard Backend Environment Variables

# Database Configuration (PostgreSQL)
ATHENA_POSTGRES_HOST=localhost
ATHENA_POSTGRES_PORT=5432
ATHENA_POSTGRES_DB=athena
ATHENA_POSTGRES_USER=postgres
ATHENA_POSTGRES_PASSWORD=postgres

# Optional: Override Athena's database config
DB_HOST=localhost
DB_PORT=5432
DB_NAME=athena
DB_USER=postgres
DB_PASSWORD=postgres
DB_MIN_POOL_SIZE=2
DB_MAX_POOL_SIZE=10

# API Configuration
API_HOST=127.0.0.1
API_PORT=8000
LOG_LEVEL=info

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost,http://127.0.0.1:3000
